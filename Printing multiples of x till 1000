import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        
        System.out.println("enter  value of x to print its multiple till 1000");
        Scanner scan = new Scanner (System.in);
        int a = scan.nextInt();
        
        for(int c=a;c<=1000;c=c+a){
        
            
    
            System.out.println(c);}
            
       
      
        
    }
}
