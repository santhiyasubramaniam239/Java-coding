import java.util.Scanner;
class Main {
    public static void main(String[] args) {
       Scanner scan= new Scanner(System.in);
       int n = scan.nextInt();
       int k= n;
        for(int i=1;i<=n;i++){ 
          
            for(int j=k;j>=1;j--){ 
           
             System.out.print(j);
             
            }
            k=k-1;
          System.out.println(" ");  
       }
    }
}
